- @page_title = "Edit member #{@member.full_name}"

= semantic_form_for @member do |f|
  
  = render :partial => 'form', :locals => {:f => f}
  
  = f.buttons do
    = f.commit_button 'Save changes'

%p
  = link_to 'Back to member record', @member
  |
  = link_to 'Back to members listing', members_path